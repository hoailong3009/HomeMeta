<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Add Mailing List with Draw" >
	<listeners>
		<listener class-name="HomeMeta.TestComponents.Listeners"/>
	</listeners>
	
	  <test thread-count="5" name="Add Mailing List with Draw Success">
    <classes>   
      <class name="HomeMeta.PageObjects.AddMailingListDraw">
		  <methods>
			          <include name="AddMailingListTest" />
		  </methods>
      </class>
    </classes>
  </test> <!-- Test -->
  
  
  <test thread-count="5" name="Add Mailing List with Draw Failed">
    <classes>    
      <class name="HomeMeta.PageObjects.AddMailingListDraw">
		  <methods>
			          <include name="AddMailingListFailed" />
		  </methods>
      </class>
    </classes>
  </test> <!-- Test -->
  
  
</suite> <!-- Suite -->
